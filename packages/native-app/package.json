{
  "name": "@meridian-event-tech/native-app",
  "version": "1.0.0",
  "description": "Meridian Events Native App - Event Survey Collection",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:dev": "expo start --dev-client",
    "android": "expo run:android",
    "preios": "node scripts/build-survey.js",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "node scripts/build-survey.js",
    "build": "expo build",
    "build:development": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "fingerprint:check": "node scripts/fingerprint.js check",
    "fingerprint:update": "node scripts/fingerprint.js update",
    "eas-build-pre-install": "node scripts/build-survey.js",
    "eas-update-pre-submit": "node scripts/build-survey.js && node scripts/fingerprint.js check",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@dr.pogodin/react-native-fs": "^2.35.1",
    "@lottiefiles/dotlottie-react": "^0.13.5",
    "@meridian-event-tech/shared": "workspace:*",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/app": "^23.2.1",
    "@react-native-firebase/auth": "^23.2.1",
    "@react-native-firebase/firestore": "^23.2.1",
    "@react-native-firebase/functions": "^23.2.1",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^6.4.1",
    "@sentry/react-native": "^7.4.0",
    "@testing-library/react-native": "^13.2.2",
