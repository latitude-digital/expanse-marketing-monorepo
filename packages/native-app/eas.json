{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "staging"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "simulator": false
      },
      "env": {
        "APP_VARIANT": "staging"
      }
    },
    "staging": {
      "distribution": "store",
      "channel": "staging",
      "ios": {
        "buildConfiguration": "Release"
      },
      "env": {
        "APP_VARIANT": "staging"
      }
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "ios": {
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "staging": {
      "ios": {
        "appleId": "shan@iotashan.com",
        "ascAppId": "YOUR_STAGING_ASC_APP_ID",
        "appleTeamId": "X234V4S5DH"
      }
    },
    "production": {
      "ios": {
        "appleId": "shan@iotashan.com",
        "ascAppId": "YOUR_PRODUCTION_ASC_APP_ID",
        "appleTeamId": "X234V4S5DH"
      }
    }
  }
}
