{
  "name": "@meridian-event-tech/shared",
  "version": "1.0.0",
  "description": "Shared TypeScript interfaces and utilities for Meridian native and web apps",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/cjs/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:watch": "bash -lc \"tsc -p tsconfig.cjs.json --watch & tsc -p tsconfig.esm.json --watch\"",
    "clean:src-artifacts": "bash -lc \"find src -type f \\\\( -name '*.js' -o -name '*.d.ts' -o -name '*.js.map' \\\\) -delete\"",
    "clean": "rm -rf lib",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "typescript",
    "shared",
    "interfaces",
    "ford",
    "lincoln",
    "survey"
  ],
  "author": "Expanse Marketing",
  "license": "UNLICENSED",
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "axios": "1.7.4",
    "react": "18.3.1",
    "survey-core": "^2.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.3",
    "@types/uuid": "^9.0.7",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
